<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="900.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane layoutX="142.0" layoutY="68.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <HBox alignment="CENTER_RIGHT" BorderPane.alignment="TOP_LEFT">
               <children>
                  <Label fx:id="progressStatus" alignment="CENTER_RIGHT" />
                  <ProgressBar fx:id="progress" prefWidth="200.0" progress="0.0" AnchorPane.rightAnchor="0.0" />
               </children>
            </HBox>
         </bottom>
         <center>
            <WebView fx:id="webview" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <HBox BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#openSettings" text="Settings" />
               </children>
            </HBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
